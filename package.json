{
	"name": "template-vite-react",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "tsx parse-data.ts && react-router dev",
		"build": "NODE_ENV=production tsx parse-data.ts && tsc -b && react-router build",
		"typecheck": "react-router typegen && tsc",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint \"app/**/*\"",
		"prettier": "prettier --write app tests",
		"prettier:ci": "prettier --check app tests",
		"test:unit": "vitest run --config ./tests/unit/vitest.config.ts",
		"test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
		"test:a11y": "playwright test --config=tests/a11y/playwright.config.ts",
		"preview": "serve build/client"
	},
	"dependencies": {
		"@react-router/node": "7.1.1",
		"@react-router/serve": "7.1.1",
		"d3": "7.9.0",
		"isbot": "5.1.20",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-router": "7.1.1",
		"zustand": "5.0.2"
	},
	"devDependencies": {
		"@axe-core/playwright": "4.10.1",
		"@playwright/test": "1.47.2",
		"@react-router/dev": "7.1.1",
		"@technologiestiftung/eslint-config": "0.3.1",
		"@technologiestiftung/prettier-config": "1.0.0",
		"@types/d3": "7.4.3",
		"@types/lodash": "4.17.14",
		"@types/node": "22.7.4",
		"@types/papaparse": "5.3.15",
		"@types/react": "19.0.3",
		"@types/react-dom": "19.0.2",
		"@vitejs/plugin-react": "4.3.2",
		"autoprefixer": "10.4.20",
		"eslint": "9.11.1",
		"eslint-plugin-react": "7.37.1",
		"eslint-plugin-react-hooks": "5.1.0",
		"eslint-plugin-react-refresh": "0.4.16",
		"globals": "15.10.0",
		"lodash": "4.17.21",
		"papaparse": "5.4.1",
		"postcss": "8.4.47",
		"prettier": "3.3.3",
		"serve": "14.2.4",
		"tailwindcss": "3.4.13",
		"tsx": "4.19.2",
		"typescript": "5.5.4",
		"vite": "5.4.8",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "2.1.1"
	}
}
