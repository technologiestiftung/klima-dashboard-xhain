{
	"name": "template-vite-react",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "tsx parse-data.ts && react-router dev",
		"build": "NODE_ENV=production tsx parse-data.ts && tsc -b && react-router build",
		"typecheck": "react-router typegen && tsc",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint \"app/**/*\"",
		"prettier": "prettier --write app tests",
		"prettier:ci": "prettier --check app tests",
		"test:unit": "vitest run --config ./tests/unit/vitest.config.ts",
		"test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
		"test:a11y": "playwright test --config=tests/a11y/playwright.config.ts",
		"preview": "serve build/client",
		"test:visual": "playwright test --config=tests/visual-regression/playwright.config.ts"
	},
	"dependencies": {
		"@floating-ui/react": "0.27.4",
		"@react-router/node": "7.1.1",
		"@react-router/serve": "7.1.1",
		"d3": "7.9.0",
		"date-fns": "4.1.0",
		"isbot": "5.1.20",
		"mapbox-gl": "3.9.4",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-router": "7.1.1",
		"zustand": "5.0.2"
	},
	"devDependencies": {
		"@axe-core/playwright": "4.10.2",
		"@playwright/test": "1.54.2",
		"@react-router/dev": "7.7.1",
		"@technologiestiftung/eslint-config": "0.4.0",
		"@technologiestiftung/prettier-config": "1.0.0",
		"@types/d3": "7.4.3",
		"@types/lodash": "4.17.20",
		"@types/mapbox-gl": "3.4.1",
		"@types/node": "22.17.0",
		"@types/papaparse": "5.3.16",
		"@types/react": "19.1.9",
		"@types/react-dom": "19.1.7",
		"@vitejs/plugin-react": "4.7.0",
		"autoprefixer": "10.4.21",
		"eslint": "9.32.0",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"globals": "15.15.0",
		"lodash": "4.17.21",
		"papaparse": "5.5.3",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"serve": "14.2.4",
		"tailwindcss": "3.4.17",
		"tsx": "4.20.3",
		"typescript": "5.9.2",
		"vite": "5.4.19",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "2.1.9"
	}
}
